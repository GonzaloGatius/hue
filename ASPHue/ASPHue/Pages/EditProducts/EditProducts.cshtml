@page
@model ASPHue.Pages.EditProdcuts.EditProductsModel
@{
    ViewData["Title"] = "Editar producto";
}
@switch (Model.Type)
{
    case "Neoprene":
    case "Hoods":
    case "Masks":
    case "Fins":
        <partial name="_ClothesEditPartial" model="Model" />
        break;
    case "BCDs":
        <partial name="_BCDsEditPartial" model="Model" />
        break;
    case "Octopus":
        <partial name="_OctopusEditPartial" model="Model" />
        break;
    case "Tanks":
        <partial name="_TanksEditPartial" model="Model" /> 
        break;
    default:
        <p>No se encontró el ítem.</p>
        break;
}
